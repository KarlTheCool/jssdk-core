{
    "plugins": ["prettier"],
    "parser": "@babel/eslint-parser",
    "extends": ["eslint:recommended", "prettier"],
    "parserOptions": {
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "es6": true,
      "jest": true,
      "jquery": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "warn",
      "no-unused-vars": 0
    }
}
  